{
  "name": "kita-catering",
  "version": "1.0.0",
  "description": "Web app with services for catering companies, specialized in kindergardens",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node server.js",
    "heroku-postbuild": "cd client && npm install --only=dev && npm install && npm run build",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "engines": {
    "node": "15.2.1"
  },
  "keywords": [
    "MERN",
    "nodeJS",
    "React",
    "Redux",
    "TailwindCss",
    "MongoDB",
    "Mongoose",
    "ExpressJS"
  ],
  "author": "KITA catering project",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-async-handler": "^1.2.0",
    "history": "^5.3.0",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "mongoose": "^6.3.6",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.4.1",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "concurrently": "^7.2.1",
    "nodemon": "^2.0.16"
  }
}
